{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "DSV-Support",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.py",
				"match": "\\b(von|beziehe|wenn|lass|inwendig|definiere|derweil|abbruch|passiere|eingabe|versuche|sonst|schlussendlich|alternativ)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.dsv",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.dsv",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "text.py"
}